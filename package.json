{
  "name": "frontend-assignment",
  "version": "1.1.0",
  "private": true,
  "main": "src/index.js",
  "scripts": {
    "start": "razzle start"
  },
  "dependencies": {
    "@emotion/core": "^11.0.0",
    "@emotion/styled": "^11.1.5",
    "@kinday/njv": "^0.4.0",
    "@reduxjs/toolkit": "^1.5.0",
    "body-parser": "^1.19.0",
    "connect": "^3.7.0",
    "express": "^4.17.1",
    "lokijs": "^1.5.11",
    "modern-normalize": "^1.0.0",
    "nanoid": "^3.1.22",
    "prop-types": "^15.7.2",
    "react": "17.0.1",
    "react-dom": "^17.0.1",
    "react-is": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-ssr-prepass": "^1.4.0"
  },
  "devDependencies": {
    "babel-preset-razzle": "^4.0.3",
    "eslint": "^7.22.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-unicorn": "^29.0.0",
    "html-webpack-plugin": "^5.3.1",
    "mini-css-extract-plugin": "^1.3.9",
    "razzle": "^4.0.3",
    "razzle-dev-utils": "^4.0.3",
    "webpack": "^5.27.1",
    "webpack-dev-server": "^3.11.2"
  },
  "eslintIgnore": [
    "build/"
  ],
  "eslintConfig": {
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "env": {
      "browser": true,
      "node": true,
      "es2021": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:react-hooks/recommended",
      "plugin:import/errors",
      "plugin:import/warnings",
      "plugin:unicorn/recommended"
    ],
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "plugins": [
      "react"
    ],
    "rules": {
      "react/jsx-uses-react": "off",
      "react/react-in-jsx-scope": "off",
      "unicorn/no-null": "off"
    }
  }
}
